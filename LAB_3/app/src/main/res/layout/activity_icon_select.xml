<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:padding="10dp"
    tools:context="ssau.lab_3.icon.IconSelectActivity">

    <ImageButton
        android:id="@+id/ic1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        app:srcCompat="@drawable/worker" />

    <ImageButton
        android:id="@+id/ic2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="1"
        android:layout_row="0"
        android:onClick="onClick"
        app:srcCompat="@drawable/food_fork_drink" />

    <ImageButton
        android:id="@+id/ic3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="2"
        android:layout_row="0"
        android:onClick="onClick"
        app:srcCompat="@drawable/hotel" />

    <ImageButton
        android:id="@+id/ic4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="3"
        android:layout_row="0"
        android:onClick="onClick"
        app:srcCompat="@drawable/delete_variant" />

    <ImageButton
        android:id="@+id/ic5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="0"
        android:layout_row="3"
        android:onClick="onClick"
        app:srcCompat="@drawable/sleep" />

    <ImageButton
        android:id="@+id/ic6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="0"
        android:layout_row="1"
        android:onClick="onClick"
        app:srcCompat="@drawable/sword_cross" />

    <ImageButton
        android:id="@+id/ic7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="1"
        android:layout_row="1"
        android:onClick="onClick"
        app:srcCompat="@drawable/dumbbell" />

    <ImageButton
        android:id="@+id/ic8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="2"
        android:layout_row="1"
        android:onClick="onClick"
        app:srcCompat="@drawable/currency_usd" />

    <ImageButton
        android:id="@+id/ic9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="3"
        android:layout_row="1"
        android:onClick="onClick"
        app:srcCompat="@drawable/car_sports" />

    <ImageButton
        android:id="@+id/ic10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="1"
        android:layout_row="3"
        android:onClick="onClick"
        app:srcCompat="@drawable/camera_party_mode" />

    <ImageButton
        android:id="@+id/ic11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="0"
        android:layout_row="2"
        android:onClick="onClick"
        app:srcCompat="@drawable/bike" />

    <ImageButton
        android:id="@+id/ic12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="1"
        android:layout_row="2"
        android:onClick="onClick"
        app:srcCompat="@drawable/baby_buggy" />

    <ImageButton
        android:id="@+id/ic13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="2"
        android:layout_row="2"
        android:onClick="onClick"
        app:srcCompat="@drawable/apple_safari" />

    <ImageButton
        android:id="@+id/ic14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="3"
        android:layout_row="2"
        android:onClick="onClick"
        app:srcCompat="@drawable/all_inclusive" />

    <ImageButton
        android:id="@+id/ic15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="2"
        android:layout_row="3"
        android:onClick="onClick"
        app:srcCompat="@drawable/account_multiple" />
</GridLayout>
